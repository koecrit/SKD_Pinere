@using Pinere.Models
@using Pinere.Helper
@using MICA.Web.Mvc.UI
@using MICA.Web.Mvc.Enumeration
@model  DataAirline
@Html.HiddenFor(o => o.Id)

<div class="form-horizontal" style="padding-right:10px;padding-left:5px">
    <div class="row" style="margin-bottom: 20px; ">
        <div class="col-md-2" style="text-align:right">
                 <button type="button" class="btn btn-primary" style="text-align:center;width:100%" data-toggle="collapse" data-target="#dvDataAirline">
                    <span class="glyphicon glyphicon-plane" aria-hidden="true"></span>&nbsp;&nbsp; Airplane Info
                 </button>
        </div>
        <div class="col-md-10"></div>
    </div>
    <div id="dvDataAirline" class="collapse">
        <div class="form-group">
            <label class="col-md-2 control-label">Nama Airline :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.NamaAirlineDis, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
            <label class="col-md-2 control-label"> Nomor Penerbangan :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.NomorPenerbangan, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label">Kota Asal :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.KotaAsalDis, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
            <label class="col-md-2 control-label">Kota Tujuan :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.KotaTujuanDis, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label">Tanggal Kedatangan :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.WaktuBerangkat, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
            <label class="col-md-2 control-label">Jumlah Penumpang :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.JumlahPenumpang, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
        </div>
        <div class="form-group">
            <label class="col-md-2 control-label">Nama Petugas :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.NamaPetugas, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
            <label class="col-md-2 control-label">Nomor Pegawai :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.NomorPetugas, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
        </div>
         <div class="form-group">
            <label class="col-md-2 control-label">Penumpang Sakit :</label>
            <div class="col-md-4">
                @Html.DisplayHiddenFor(o => o.PenumpangSakitDis, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
            <label class="col-md-2 control-label">Jumlah Sakit :</label>
            <div class="col-md-4">
                  @Html.DisplayHiddenFor(o => o.JumlahSakit, new { @class = "form-control input-sm",@readonly = "readonly" })
            </div>
        </div>
           <div class="form-group">
            <label class="col-md-2 control-label">Form GenDec. :</label>
            <div class="col-md-4">
                <label class="form-control input-sm" readonly="readonly">@Html.Raw(Model.FormGendecLink)</label>
            </div>
        </div>
    </div>
    
</div>
    
